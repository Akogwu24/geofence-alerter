{
  "expo": {
    "name": "geofence-alarter",
    "slug": "geofence-alarter",
    "scheme": "geofence-alarter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/etap.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#278ea5"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCv-erNK4YaOa0exUbxtd57EN7YUqYBLIQ"
      },
      "bundleIdentifier": "com.akogwu.geofencealarter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/etap.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCv-erNK4YaOa0exUbxtd57EN7YUqYBLIQ"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.akogwu.geofencealarter"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e5c83d6-3231-4626-9cbe-028402441862"
      }
    }
  }
}
